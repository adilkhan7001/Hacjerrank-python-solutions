if __name__ == '__main__':
   
    x = int(input())
    y = int(input())
    z = int(input())
    n = int(input())
    #abc=[]
    output=[]
    for x in range(0,x+1):
        for y in range (0,y+1):
            for z in range(0,z+1):
              if(x+y+z)!=n:
                abc=[x,y,z] 
                output.append(abc)
                
    
print(output) 
